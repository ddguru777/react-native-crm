{
  "name": "api_gateway",
  "description": "description",
  "version": "1.0.0",
  "main": "./tools/run.js",
  "author": "niten2",
  "license": "ISC",
  "keywords": [
    "node"
  ],
  "scripts": {
    "start": "nodemon --exec node --harmony --require ./tools/babelhook ./tools/run.js | ./node_modules/.bin/bunyan -o short -l trace",
    "seed": "node --require ./tools/babelhook ./db/seed.js",
    "console": "node --require ./tools/babelhook ./tools/console.js",
    "db:migrate": "node_modules/.bin/sequelize db:migrate --migrations-path=db/migrations",
    "db:migration": "node_modules/.bin/sequelize migration:create --migrations-path=db/migrations",
    "db:seed": "node_modules/.bin/sequelize db:seed:all --seeders-path=db/seeders",
    "db:seed:client": "node --require ./tools/babelhook ./api/services/clients/createClientForCSV.js",
    "db:drop": "rm -rf db/database.dev.sqlite",
    "db:reset": "npm run db:drop; npm run db:migrate; npm run db:seed;",
    "tests": "NODE_ENV=test node_modules/.bin/_mocha --timeout 10000 --compilers js:babel-register $(find api -name '*.spec.js')",
    "test": "NODE_ENV=test nodemon node_modules/.bin/_mocha --timeout 10000 --compilers js:babel-register",
    "testing": "NODE_ENV=test nodemon --exec mocha test --inspect-brk --require tools/babelhook test/testing.js",
    "dashboard": "nodemon --exec 'nodejs-dashboard babel-node' src/index.js"
  },
  "dependencies": {
    "apollo-fetch": "^0.6.0",
    "apollo-server-express": "^1.0.4",
    "body-parser": "~1.17.2",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.1",
    "dotenv": "^4.0.0",
    "express": "^4.12.4",
    "express-bunyan-logger": "^1.3.1",
    "express-jwt": "^5.1.0",
    "graphql": "^0.11.2",
    "graphql-helpers": "0.0.11",
    "graphql-sequelize": "^5.4.2",
    "graphql-tools": "^1.1.0",
    "ramda": "^0.24.1",
    "sequelize": "^4.8.0",
    "sequelize-cli": "^2.5.1",
    "sqlite": "^2.3.0",
    "bunyan": "^1.8.10"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-module-resolver": "^2.7.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "babel-register": "^6.26.0",
    "babel-resolver": "^1.1.0",
    "chai": "^4.1.1",
    "mocha": "^3.5.0",
    "nodemon": "^1.11.0"
  }
}
