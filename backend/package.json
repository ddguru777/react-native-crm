{
  "name": "backend",
  "description": "",
  "version": "2.0.0",
  "main": "",
  "author": "niten2",
  "license": "ISC",
  "keywords": [
    "node"
  ],
  "scripts": {
    "start": "node_modules/.bin/nodemon --exec node --harmony ./tools/run.js | ./node_modules/.bin/bunyan -o short -l trace",
    "console": "node --require ./tools/babelhook ./tools/console.js",
    "db:migrate": "node_modules/.bin/sequelize db:migrate",
    "db:migrate:test": "NODE_ENV=test node_modules/.bin/sequelize db:migrate",
    "db:migration": "node_modules/.bin/sequelize migration:create",
    "db:seed": "node_modules/.bin/sequelize db:seed:all",
    "db:drop": "rm -rf db/database.development.sqlite",
    "db:drop:test": "rm -rf db/database.test.sqlite",
    "db:reset": "npm run db:drop; npm run db:migrate; npm run db:seed;",
    "db:reset:test": "npm run db:drop:test; npm run db:migrate:test",
    "tests": "NODE_ENV=test node_modules/.bin/_mocha --timeout 10000 --compilers js:babel-register ./**/*.spec.js",
    "test": "NODE_ENV=test node_modules/.bin/nodemon node_modules/.bin/_mocha --timeout 10000 --compilers js:babel-register",
    "vk:friend": "babel-node ./api/services/vk/io.js",
    "vk:test": "babel-node ./api/services/vk/test.js"
  },
  "dependencies": {
    "apollo-fetch": "^0.7.0",
    "apollo-server-express": "^1.3.5",
    "body-parser": "~1.18.2",
    "bunyan": "^1.8.12",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "graphql": "^0.13.2",
    "graphql-request": "^1.5.1",
    "graphql-tools": "^2.24.0",
    "jsonwebtoken": "^8.2.1",
    "phantom": "^4.0.12",
    "phantomjs": "^2.1.7",
    "phantomjs-prebuilt": "^2.1.16",
    "ramda": "^0.25.0",
    "selenium-webdriver": "^4.0.0-alpha.1",
    "sequelize": "^4.37.6",
    "sequelize-cli": "^4.0.0",
    "sqlite": "^2.9.1",
    "vk-io": "^4.0.0-beta.6"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-resolver": "^1.1.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "dotenv": "^5.0.1",
    "fetch-mock": "^6.3.0",
    "mocha": "^5.1.1",
    "nock": "^9.2.5",
    "nodemon": "^1.17.3",
    "supertest": "^3.0.0"
  }
}
