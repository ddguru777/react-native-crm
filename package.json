{
  "name": "backend_blog_nodejs",
  "scripts": {
    "start": "nodemon --exec node --harmony --require ./tools/babelhook index.js | ./node_modules/.bin/bunyan -o short -l trace",
    "seed": "node --require ./tools/babelhook ./db/seed.js",
    "console": "node --require ./tools/babelhook ./tools/console.js",
    "test": "mocha test --inspect-brk --require tools/babelhook",
    "db:migrate": "node_modules/.bin/sequelize db:migrate --migrations-path=db/migrations",
    "db:migration": "node_modules/.bin/sequelize migration:create --migrations-path=db/migrations",
    "db:seed": "node_modules/.bin/sequelize db:seed:all --seeders-path=db/seeders",
    "db:drop": "rm -rf db/database.dev.sqlite",
    "db:reset": "npm run db:drop; npm run db:migrate; npm run db:seed;",
    "parse:test": "nodemon --exec node --require ./tools/babelhook api/lib/vk/parse_tests.js"
  },
  "dependencies": {
    "async": "https://registry.npmjs.org/async/-/async-0.9.2.tgz",
    "basic-auth-connect": "^1.0.0",
    "body-parser": "~1.12.4",
    "chai": "^3.5.0",
    "chai-http": "^2.0.1",
    "cookie-parser": "~1.3.5",
    "cors": "^2.8.1",
    "debug": "~2.2.0",
    "dotenv": "^1.1.0",
    "express": "~4.12.4",
    "express-async-router": "^0.1.12",
    "express-bunyan-logger": "^1.3.1",
    "express-jwt": "^5.1.0",
    "json-api": "^2.15.7",
    "jsonapi-serializer": "^3.4.1",
    "jsonwebtoken": "^7.1.9",
    "left-pad": "^1.1.3",
    "lodash": "^4.17.4",
    "method-override": "^2.3.3",
    "mocha": "^2.4.5",
    "mocha-babel": "^3.0.3",
    "moment": "^2.15.1",
    "mongodb-uri": "^0.9.7",
    "mongoose": "^4.6.1",
    "mongoose-timestamp": "^0.4.0",
    "mongoose-url-slugs": "^0.1.3",
    "morgan": "~1.5.3",
    "nodemon": "^1.10.2",
    "passport": "^0.3.2",
    "passport-google-oauth": "^1.0.0",
    "passport-jwt": "^2.2.1",
    "sequelize": "^3.30.2",
    "sequelize-cli": "^2.5.1",
    "serve-favicon": "~2.2.1",
    "sqlite": "^2.3.0",
    "vksdk": "^5.3.2"
  },
  "devDependencies": {
    "babel": "^5.4.7",
    "babel-cli": "^6.14.0",
    "babel-core": "^6.11.4",
    "babel-eslint": "^6.1.2",
    "babel-plugin-syntax-async-functions": "https://registry.npmjs.org/babel-plugin-syntax-async-functions/-/babel-plugin-syntax-async-functions-6.13.0.tgz",
    "babel-plugin-transform-async-to-generator": "^6.16.0",
    "babel-polyfill": "https://registry.npmjs.org/babel-polyfill/-/babel-polyfill-6.16.0.tgz",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-0": "^6.22.0",
    "babel-register": "https://registry.npmjs.org/babel-register/-/babel-register-6.18.0.tgz",
    "babel-resolver": "^1.1.0",
    "bunyan": "^1.8.10",
    "debug": "https://registry.npmjs.org/debug/-/debug-2.2.0.tgz"
  }
}
