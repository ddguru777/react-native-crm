{
  "name": "crm_gateway",
  "description": "description",
  "version": "1.0.0",
  "main": "./tools/run.js",
  "author": "niten2",
  "license": "ISC",
  "keywords": [
    "node"
  ],
  "scripts": {
    "start": "node_modules/.bin/nodemon --exec node --harmony --require ./tools/babelhook ./tools/run.js | ./node_modules/.bin/bunyan -o short -l trace",
    "seed": "node --require ./tools/babelhook ./db/seed.js",
    "console": "node --require ./tools/babelhook ./tools/console.js",
    "db:migrate": "node_modules/.bin/sequelize db:migrate",
    "db:migrate:test": "NODE_ENV=test node_modules/.bin/sequelize db:migrate",
    "db:migration": "node_modules/.bin/sequelize migration:create",
    "db:seed": "node_modules/.bin/sequelize db:seed:all",
    "db:drop": "rm -rf db/database.dev.sqlite",
    "db:drop:test": "rm -rf db/database.test.sqlite",
    "db:reset": "npm run db:drop; npm run db:migrate; npm run db:seed;",
    "db:reset:test": "npm run db:drop:test; npm run db:migrate:test",
    "tests": "NODE_ENV=test node_modules/.bin/_mocha --timeout 10000 --compilers js:babel-register $(find api -name '*.spec.js')",
    "test": "NODE_ENV=test node_modules/.bin/nodemon node_modules/.bin/_mocha --timeout 10000 --compilers js:babel-register"
  },
  "dependencies": {
    "babyparse": "^0.4.6",
    "apollo-server-express": "^1.0.4",
    "body-parser": "~1.12.4",
    "bunyan": "^1.8.10",
    "cors": "^2.8.3",
    "express": "~4.12.4",
    "graphql": "^0.10.3",
    "graphql-tools": "^1.0.0",
    "ramda": "^0.23.0",
    "ramda-fantasy": "^0.8.0",
    "sequelize": "^3.30.2",
    "sequelize-cli": "^2.5.1",
    "sqlite": "^2.3.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-resolver": "^1.1.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.1",
    "chai-as-promised": "^7.1.1",
    "database-cleaner": "^1.2.0",
    "dotenv": "^4.0.0",
    "mocha": "^3.5.0",
    "nodemon": "^1.11.0"
  }
}
